      $LAPSINSTALLROOT/etc/verif/cloud_forecast.csh $LAPS_DATA_ROOT $N_VERIF_TIMES  $DOMAIN_NAME comp  > $LAPS_DATA_ROOT/log/cloud_fcst_comp.log

