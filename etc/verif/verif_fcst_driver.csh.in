        setenv COMP_LINK radar_fcst_all_$COMP.png
        if (-e $IMOUT) then
            echo "cd $IMGDIR; rm -f $COMP_LINK; ln -s radar_fcst_all_$COMP\_$LAPS_A9TIME.png $COMP_LINK"
